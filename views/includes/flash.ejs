<% if(typeof success !=="undefined" && success.length >0){ %>
  <div class="alert alert-success  alert-dismissible fade show col-6 offset-3"><%=success[0]%>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>

<%}%>

<% if( typeof error !== 'undefined' && error.length >0){ %>
  <div class="alert alert-danger  alert-dismissible fade show col-6 offset-3"><%=error[0]%>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>

<%}%>